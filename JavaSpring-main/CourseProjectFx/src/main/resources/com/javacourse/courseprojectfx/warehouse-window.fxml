<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javacourse.courseprojectfx.fxControllers.WarehouseController">
    <children>
        <TextField fx:id="addressTextField" layoutX="15.0" layoutY="32.0" promptText="Address" />
        <ComboBox fx:id="cityComboBox" layoutX="14.0" layoutY="74.0" prefWidth="150.0" promptText="City" />
        <ListView fx:id="managerList" layoutX="174.0" layoutY="7.0" prefHeight="200.0" prefWidth="163.0" />
        <ListView fx:id="productList" layoutX="174.0" layoutY="207.0" prefHeight="187.0" prefWidth="163.0" />
        <ListView fx:id="managerAddList" layoutX="342.0" layoutY="7.0" prefHeight="200.0" prefWidth="163.0" />
        <ListView fx:id="productAddList" layoutX="342.0" layoutY="207.0" prefHeight="187.0" prefWidth="163.0" />
        <Button layoutX="524.0" layoutY="175.0" mnemonicParsing="false" onAction="#removeManager" text="Remove" />
        <Button layoutX="524.0" layoutY="150.0" mnemonicParsing="false" onAction="#addManager" text="Add" />
        <Button layoutX="524.0" layoutY="361.0" mnemonicParsing="false" onAction="#removeProduct" text="Remove" />
        <Button layoutX="524.0" layoutY="336.0" mnemonicParsing="false" onAction="#addProduct" text="Add" />
        <Button fx:id="createButton" layoutX="64.0" layoutY="125.0" mnemonicParsing="false" onAction="#createWarehouse" text="Create" />
    </children>
</AnchorPane>
